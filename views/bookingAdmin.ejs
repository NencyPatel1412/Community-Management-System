  <div style="padding:4%; padding-top: 0%; font-family:sans-serif; font-size:15px;">
    <h2 style="background-color:rgb(51, 51, 51); padding:18px;color:white; text-align: center; border-radius: 1rem; font-size: 20px; max-width: 1000px; margin:auto; margin-bottom: 10px;">
      Bookings
    </h2>
    <div class="container container-profile">
      <row>
        <col-4>
            <a href="/booking" class="btn btn-danger btn-md text-uppercase center" style="border-radius:3rem; max-width:180px; display: block; margin: auto;margin-top: 15px;margin-bottom: 15px;">
              Book Now
            </a>
        </col-4>
      </row>
      <div class="row" id="bookinh-table">
  <table class="bill-content">
    <thead>
      <tr>
        <th>firstName</th>
        <th>lastName</th>
        <th>flatNumber</th>
        <th>Date</th>
        <th>Start Time</th>
        <th>End Time</th>
        <th>Purpose</th>
       
      </tr>
    </thead>
    <tbody >
      <% for (let booking of bookings) { %>
        <tr>
          <td><%= booking.firstName%></td>
          <td><%= booking.lastName%></td>
          <td><%= booking.flatNumber%></td>
          <td><%= booking.date %></td>
          <td><%= booking.start_time %></td>
          <td><%= booking.end_time %></td>
          <td><%= booking.purpose %></td>
          
          
        </tr>
      <% } %>
    </tbody>
  </table>
<!-- </div>     -->
</div>    
</div>
</div>
</div>
   
