  <div style="padding:4%; padding-top: 0%; font-family:sans-serif; font-size:15px;">
    <h2 style="background-color:rgb(51, 51, 51); padding:18px;color:white; text-align: center; border-radius: 1rem; font-size: 20px; max-width: 1000px; margin:auto; margin-bottom: 10px;">
      Car Parking Allocation
    </h2>
    <div class="container container-profile">
      
      <row>
        <col-4>
            <a href="/allocatecar" class="btn btn-danger btn-md text-uppercase center" style="border-radius:3rem; max-width:180px; display: block; margin: auto;margin-top: 15px;margin-bottom: 15px;">
              Allocate Now
            </a>
        </col-4>
      </row> 
      <div class="row">
  <table class="bill-content">
    <thead>
      <tr>
        <th>firstName</th>
        <th>lastName</th>
        <th>flatNumber</th>
        <th>car_number</th>
        <th>parkingNumber</th>
        
      </tr>
    </thead>
    <tbody>
      <% for (let car of cars) { %>
        <tr>
          <td><%= car.firstName%></td>
          <td><%= car.lastName%></td>
          <td><%= car.flatNumber%></td>
          <td><%= car.car_number %></td>
          <td><%= car.parkingNumber %></td>
         

        
          
          
        </tr>
      <% } %>
    </tbody>
  </table>
</div>    
</div>    
</div>
</div>
   
